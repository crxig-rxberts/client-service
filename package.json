{
  "name": "client-service",
  "version": "1.0.0",
  "description": "Service for managing CLIENT user type data",
  "main": "src/server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "lint": "eslint .",
    "docker:build": "docker build -t client-service .",
    "docker:run": "docker run -p 3008:3008 -d client-service",
    "docker:run:local": "docker run --name client-service --network bookit-network -p 3008:3008 -e AWS_ENDPOINT=http://dynamodb-local:8000 -e AWS_REGION=local -d client-service",
    "docker:stop": "docker stop client-service",
    "docker:clean": "docker rm client-service"
  },
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@jest/globals": "^29.7.0",
    "eslint": "^9.11.1",
    "eslint-plugin-jest": "^28.8.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0"
  }
}
